{
  "name": "storybook-addon-baseline",
  "version": "0.0.1",
  "description": "A Storybook addon that shows Baseline compatibility badges for web features",
  "keywords": [
    "storybook-addons",
    "baseline",
    "web-features",
    "compatibility",
    "browser-support"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/veiko/storybook-addon-baseline"
  },
  "type": "module",
  "license": "MIT",
  "author": "Veronica Carrillo-Marquez",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./preview": {
      "types": "./dist/index.d.ts",
      "import": "./dist/preview.js",
      "require": "./dist/preview.cjs"
    },
    "./manager": "./dist/manager.js",
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "build": "tsup",
    "build:features": "node scripts/generate-features-snapshot.mjs",
    "build:storybook": "cd docs && storybook build",
    "build:watch": "npm run build -- --watch",
    "dev": "cd docs && storybook dev -p 6006",
    "docs:build": "cd docs && storybook build",
    "prerelease": "zx scripts/prepublish-checks.js",
    "release": "npm run build && auto shipit"
  },
  "dependencies": {
    "@storybook/icons": "^1.4.0",
    "web-features": "^3.3.0"
  },
  "peerDependencies": {
    "storybook": "^9.1.0"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^9.1.0",
    "@storybook/react-vite": "^9.1.0",
    "@types/css-tree": "^2.3.11",
    "@types/node": "^22",
    "@types/react": "^18.2.65",
    "@types/react-dom": "^18.2.21",
    "@vitejs/plugin-react": "^4.2.1",
    "auto": "^11.1.1",
    "css-tree": "^3.1.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.3.3",
    "prompts": "^2.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": ">=9.0.0-0 <10.0.0-0",
    "tsup": "^8.2.4",
    "typescript": "^5.8.2",
    "vite": "^5.3.5",
    "vite-plugin-svgr": "^4.5.0",
    "zx": "^8.4.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "bundler": {
    "exportEntries": [
      "src/index.ts"
    ],
    "managerEntries": [
      "src/manager.tsx"
    ],
    "previewEntries": [
      "src/preview.ts"
    ]
  },
  "storybook": {
    "displayName": "Baseline",
    "supportedFrameworks": [
      "react",
      "vue",
      "angular",
      "html",
      "web-components"
    ],
    "icon": "https://web.dev/static/baseline/image/baseline-wordmark_480.png"
  },
  "packageManager": "yarn@4.9.1"
}
